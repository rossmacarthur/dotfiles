{
    "[git-commit]": {
        "editor.rulers": [
            50,
            72
        ]
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.formatOnSave": true,
        "editor.rulers": [
            80
        ]
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "[rust]": {
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.rulers": [
            80
        ],
        "editor.tabSize": 4
    },
    "editor.cursorWidth": 4,
    "editor.folding": true,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 13.5,
    "editor.formatOnType": true,
    "editor.inlayHints.enabled": "off",
    "editor.inlayHints.fontSize": 10,
    "editor.inlineSuggest.enabled": true,
    "editor.language.colorizedBracketPairs": [],
    "editor.lineHeight": 0,
    "editor.minimap.enabled": false,
    "editor.mouseWheelScrollSensitivity": 2,
    "editor.renderWhitespace": "selection",
    "editor.rulers": [
        100
    ],
    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "*.mutable": {
                "underline": false
            }
        }
    },
    "editor.smoothScrolling": true,
    "editor.unicodeHighlight.includeStrings": false,
    "editor.wordWrap": "off",
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.env.*": "shellscript",
        "*.plist": "xml",
        "Dockerfile.*": "dockerfile",
        "{.in,.out}": "shellscript"
    },
    "files.exclude": {
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/__pycache__": true,
        ".*_cache": true,
        ".coverage*": true,
        ".python-version": true,
        "target": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.codeLens.enabled": false,
    "go.toolsEnvVars": {
        "GO111MODULE": "on"
    },
    "gopls": {
        "ui.navigation.importShortcut": "Definition"
    },
    "python.defaultInterpreterPath": "${env:HOME}/.pyenv/versions/${workspaceFolderBasename}/bin/python",
    "python.languageServer": "Pylance",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "rust-analyzer.completion.snippets.custom": {
        "Mutex::arc": {
            "body": [
                "Arc::new(Mutex::new(${receiver}))"
            ],
            "postfix": "mutex",
            "requires": [
                "std::sync::Arc",
                "std::sync::Mutex"
            ],
            "scope": "expr"
        }
    },
    "rust-analyzer.diagnostics.experimental.enable": true,
    "rust-analyzer.imports.granularity.group": "item",
    "rust-analyzer.server.extraEnv": {
        "CARGO_TARGET_DIR": "target/rust-analyzer"
    },
    "search.quickOpen.includeHistory": false,
    "security.workspace.trust.enabled": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.env.osx": {
        "EDITOR": "code --wait",
        "GIT_EDITOR": "code --wait"
    },
    "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.mouseWheelScrollSensitivity": 2,
    "workbench.activityBar.visible": true,
    "workbench.colorCustomizations": {
        "editorGutter.background": "#252525",
        "editorInlayHint.background": "#1e1e1e",
        "editorInlayHint.foreground": "#6a6a6a",
        "terminal.ansiBlue": "#569cd6"
    },
    "workbench.editor.languageDetection": false,
    "workbench.editorAssociations": {
        "git-rebase-todo": "default"
    },
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "readme"
}
