{
    "[git-commit]": {
        "editor.rulers": [
            50,
            72
        ],
        "editor.wordWrap": "off"
    },
    "[go]": {
        "editor.rulers": [
            80,
            100,
            120
        ]
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.formatOnSave": true
    },
    "[python]": {
        "editor.rulers": [
            80,
            100
        ]
    },
    "[rust]": {
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "breadcrumbs.enabled": false,
    "editor.cursorWidth": 4,
    "editor.folding": false,
    "editor.fontFamily": "Source Code Pro for Powerline",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.formatOnType": true,
    "editor.lineHeight": 0,
    "editor.minimap.enabled": false,
    "editor.mouseWheelScrollSensitivity": 2,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "selection",
    "editor.rulers": [
        80
    ],
    "editor.smoothScrolling": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "string.quoted.docstring.multi.python",
                "settings": {
                    "foreground": "#6A9955"
                }
            }
        ]
    },
    "explorer.openEditors.visible": 0,
    "files.associations": {
        "*.env.*": "shellscript",
        "*.plist": "xml",
        "Dockerfile.*": "dockerfile",
        "{.in,.out}": "shellscript"
    },
    "files.exclude": {
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/__pycache__": true,
        ".*_cache": true,
        ".coverage*": true,
        ".python-version": true,
        "target": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.codeLens.enabled": false,
    "go.toolsManagement.autoUpdate": true,
    "gopls": {
        "ui.navigation.importShortcut": "Definition"
    },
    "markdown.extension.toc.level": "2..6",
    "python.defaultInterpreterPath": "${env:HOME}/.pyenv/versions/${workspaceFolderBasename}/bin/python",
    "python.languageServer": "Pylance",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "rust-analyzer.assist.importEnforceGranularity": true,
    "rust-analyzer.assist.importGranularity": "module",
    "rust-analyzer.assist.importPrefix": "crate",
    "rust-analyzer.completion.autoimport.enable": true,
    "rust-analyzer.diagnostics.disabled": [
        "inactive-code"
    ],
    "rust-analyzer.diagnostics.enableExperimental": false,
    "rust-analyzer.experimental.procAttrMacros": true,
    "rust-analyzer.inlayHints.enable": false,
    "rust-analyzer.lens.implementations": false,
    "rust-analyzer.updates.askBeforeDownload": false,
    "search.quickOpen.includeHistory": false,
    "security.workspace.trust.enabled": false,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.env.osx": {
        "EDITOR": "code --wait",
        "GIT_EDITOR": "code --wait"
    },
    "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.mouseWheelScrollSensitivity": 2,
    "workbench.colorCustomizations": {
        "terminal.ansiBlue": "#569cd6"
    },
    "workbench.editor.languageDetection": false,
    "workbench.editorAssociations": {
        "git-rebase-todo": "default"
    },
    "workbench.panel.defaultLocation": "right",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "readme"
}
