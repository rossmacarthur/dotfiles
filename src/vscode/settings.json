{
    "[git-commit]": {
        "editor.rulers": [
            50,
            72
        ]
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.formatOnSave": true
    },
    "[rust]": {
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "editor.cursorWidth": 4,
    "editor.folding": false,
    "editor.fontFamily": "Source Code Pro for Powerline",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.formatOnType": true,
    "editor.lineHeight": 0,
    "editor.minimap.enabled": false,
    "editor.mouseWheelScrollSensitivity": 2,
    "editor.renderWhitespace": "selection",
    "editor.rulers": [
        100
    ],
    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "*.mutable": {
                "underline": false
            }
        }
    },
    "editor.smoothScrolling": true,
    "editor.wordWrap": "off",
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.env.*": "shellscript",
        "*.plist": "xml",
        "Dockerfile.*": "dockerfile",
        "{.in,.out}": "shellscript"
    },
    "files.exclude": {
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/__pycache__": true,
        ".*_cache": true,
        ".coverage*": true,
        ".python-version": true,
        "target": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.codeLens.enabled": false,
    "go.toolsManagement.autoUpdate": true,
    "gopls": {
        "ui.navigation.importShortcut": "Definition"
    },
    "markdown.extension.toc.level": "2..6",
    "python.defaultInterpreterPath": "${env:HOME}/.pyenv/versions/${workspaceFolderBasename}/bin/python",
    "python.languageServer": "Pylance",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "rust-analyzer.assist.importEnforceGranularity": true,
    "rust-analyzer.assist.importGranularity": "module",
    "rust-analyzer.assist.importPrefix": "crate",
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.completion.autoimport.enable": true,
    "rust-analyzer.completion.snippets": {
        "arc": {
            "body": [
                "Arc::new(${receiver})"
            ],
            "postfix": "arc",
            "requires": "std::sync::Arc",
            "scope": "expr"
        },
        "mutex": {
            "body": [
                "Arc::new(Mutex::new(${receiver}))"
            ],
            "postfix": "mutex",
            "requires": [
                "std::sync::Arc",
                "std::sync::Mutex"
            ],
            "scope": "expr"
        },
        "rc": {
            "body": [
                "Rc::new(${receiver})"
            ],
            "postfix": "rc",
            "requires": "std::rc::Rc",
            "scope": "expr"
        }
    },
    "rust-analyzer.diagnostics.disabled": [
        "inactive-code"
    ],
    "rust-analyzer.diagnostics.enableExperimental": true,
    "rust-analyzer.experimental.procAttrMacros": true,
    "rust-analyzer.inlayHints.enable": false,
    "rust-analyzer.server.extraEnv": {
        "CARGO_TARGET_DIR": "target/rust-analyzer"
    },
    "search.quickOpen.includeHistory": false,
    "security.workspace.trust.enabled": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.env.osx": {
        "EDITOR": "code --wait",
        "GIT_EDITOR": "code --wait"
    },
    "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.mouseWheelScrollSensitivity": 2,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "terminal.ansiBlue": "#569cd6",
        "terminal.background": "#1e1e1e"
    },
    "workbench.colorTheme": "Default Dark+",
    "workbench.editor.languageDetection": false,
    "workbench.editorAssociations": {
        "git-rebase-todo": "default"
    },
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "readme"
}
